{
    "name": "loopback-acl-extension",
    "version": "6.3.0",
    "description": "Loopback ACL generic implementation based on RBAC",
    "keywords": [
        "loopback-extension",
        "loopback"
    ],
    "main": "index.js",
    "engines": {
        "node": ">=8.9"
    },
    "scripts": {
        "clean": "lb-clean dist build *.tsbuildinfo",
        "prebuild": "npm run clean",
        "build": "lb-tsc --outDir dist",
        "build:watch": "lb-tsc --watch --outDir dist",
        "start": "node .",
        "start:watch": "nodemon .",
        "test": "lb-mocha --allow-console-logs \"test/**/*.test.ts\"",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/koliberr136a1/loopback-acl-extension"
    },
    "author": "KoLiBer",
    "license": "MIT",
    "files": [
        "README.md",
        "index.js",
        "index.d.ts",
        "dist"
    ],
    "dependencies": {
        "@loopback/authentication": "^4.1.1",
        "@loopback/boot": "^2.0.2",
        "@loopback/context": "^3.2.0",
        "@loopback/core": "^2.2.0",
        "@loopback/openapi-v3": "^3.1.1",
        "@loopback/repository": "^2.0.2",
        "@loopback/rest": "^3.1.0",
        "@loopback/rest-explorer": "^2.0.2",
        "@loopback/security": "^0.2.2",
        "@types/request-ip": "0.0.34",
        "apollo-server": "^2.11.0",
        "graphql": "^14.6.0",
        "loopback-authorization-extension": "^11.1.0",
        "loopback-history-extension": "^6.1.0",
        "openapi-to-graphql": "^1.6.3",
        "randomatic": "^3.1.1",
        "request-ip": "^2.1.3",
        "tslib": "^1.11.1"
    },
    "devDependencies": {
        "@loopback/build": "^5.0.0",
        "@loopback/testlab": "^2.0.2",
        "@types/node": "^10.17.17",
        "nodemon": "^2.0.2",
        "source-map-support": "^0.5.16",
        "typescript": "~3.8.3"
    }
}
