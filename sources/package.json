{
    "name": "loopback-acl-extension",
    "version": "4.1.0",
    "description": "Loopback ACL generic implementation based on RBAC",
    "keywords": [
        "loopback-extension",
        "loopback"
    ],
    "main": "index.js",
    "engines": {
        "node": ">=8.9"
    },
    "scripts": {
        "clean": "lb-clean dist build *.tsbuildinfo",
        "prebuild": "npm run clean",
        "build": "lb-tsc --outDir dist",
        "postbuild": "parcel build --out-dir build --cache-dir build/cache --target node dist/index.js",
        "build:watch": "lb-tsc --watch --outDir dist",
        "start": "node .",
        "start:watch": "nodemon .",
        "test": "lb-mocha --allow-console-logs \"test/**/*.test.ts\"",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/koliberr136a1/loopback-acl-extension"
    },
    "author": "KoLiBer",
    "license": "MIT",
    "files": [
        "README.md",
        "index.js",
        "index.d.ts",
        "dist"
    ],
    "dependencies": {
        "@loopback/authentication": "^3.2.3",
        "@loopback/boot": "^1.5.12",
        "@loopback/context": "^1.24.0",
        "@loopback/core": "^1.11.0",
        "@loopback/openapi-v3": "^1.10.2",
        "@loopback/repository": "^1.15.5",
        "@loopback/rest": "^1.24.0",
        "@loopback/rest-explorer": "^1.3.4",
        "@loopback/security": "^0.1.8",
        "apollo-server": "^2.9.12",
        "graphql": "^14.5.8",
        "loopback-authorization-extension": "latest",
        "loopback-history-extension": "latest",
        "openapi-to-graphql": "^1.6.3",
        "randomatic": "^3.1.1"
    },
    "devDependencies": {
        "@loopback/build": "^2.1.0",
        "@loopback/testlab": "^1.9.5",
        "@types/node": "^10.17.5",
        "nodemon": "^2.0.1",
        "parcel-bundler": "^1.12.4",
        "source-map-support": "^0.5.16",
        "typescript": "~3.7.2"
    }
}
