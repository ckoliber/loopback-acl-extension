{
    "name": "loopback-acl-extension",
    "version": "5.18.0",
    "description": "Loopback ACL generic implementation based on RBAC",
    "keywords": [
        "loopback-extension",
        "loopback"
    ],
    "main": "index.js",
    "engines": {
        "node": ">=8.9"
    },
    "scripts": {
        "clean": "lb-clean dist build *.tsbuildinfo",
        "prebuild": "npm run clean",
        "build": "lb-tsc --outDir dist",
        "build:watch": "lb-tsc --watch --outDir dist",
        "start": "node .",
        "start:watch": "nodemon .",
        "test": "lb-mocha --allow-console-logs \"test/**/*.test.ts\"",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/koliberr136a1/loopback-acl-extension"
    },
    "author": "KoLiBer",
    "license": "MIT",
    "files": [
        "README.md",
        "index.js",
        "index.d.ts",
        "dist"
    ],
    "dependencies": {
        "@loopback/authentication": "^3.3.1",
        "@loopback/boot": "^1.7.2",
        "@loopback/context": "^2.0.0",
        "@loopback/core": "^1.12.2",
        "@loopback/openapi-v3": "^1.12.0",
        "@loopback/repository": "^1.18.0",
        "@loopback/rest": "^1.26.0",
        "@loopback/rest-explorer": "^1.4.8",
        "@loopback/security": "^0.1.11",
        "@types/request-ip": "0.0.34",
        "apollo-server": "^2.9.16",
        "graphql": "^14.6.0",
        "loopback-authorization-extension": "^10.3.0",
        "loopback-history-extension": "^5.5.0",
        "openapi-to-graphql": "^1.6.3",
        "randomatic": "^3.1.1",
        "request-ip": "^2.1.3"
    },
    "devDependencies": {
        "@loopback/build": "^3.1.0",
        "@loopback/testlab": "^1.10.2",
        "@types/node": "^13.7.0",
        "nodemon": "^2.0.1",
        "source-map-support": "^0.5.16",
        "typescript": "~3.7.5"
    }
}
